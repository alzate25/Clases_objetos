package Clases_Objetos;

public class Animal_salvaje {
    
    String color;
    String escamas;
    String lengua;
    String patas;
    double tama単o;
    int peso;
    
    public static void main(String[] args){
        
        Animal_salvaje serpiente=new Animal_salvaje();
        serpiente.color="Verde y rojo";
        serpiente.peso=4;
        serpiente.tama単o=2.5;
        serpiente.patas = "No tiene";
        serpiente.escamas="Tiene";
        serpiente.lengua="Lengua bifida";
        System.out.println("Serpiente\n\n"+"Color:\t"+serpiente.color+"\n"+"Kg:\t"+serpiente.peso+"\n"+"Tama単o:\t"+serpiente.tama単o+"\n"+"Patas:\t"+serpiente.patas+"\n"+"Escamas:"+serpiente.escamas+"\n"+"Lengua:\t"+serpiente.lengua+"\n");
    }
}
