        SITP.tamaño=10.6;
        SITP.carac_1="Algunas veces mas rapido";
        SITP.carac_2="Lleno en hora pico";
        System.out.println("(4) SITP:\n\n"+"Color:\t"+SITP.color+"\n"+"Tamaño(m):\t"+SITP.tamaño+"\n"+SITP.carac_1+"\n"+SITP.carac_2+"\n");
        
        //Moto
