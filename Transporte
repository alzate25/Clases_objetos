package Clases_Objetos;

public class Trans_publico {
    String color;
    String carac_1;
    String carac_2;
    String carac_3;
    double tamaño;
    
    public static void main (String [] args){
        
        //Transmilenio
        Trans_publico transmilenio = new Trans_publico();
        
        transmilenio.color="Rojo";
        transmilenio.tamaño=27;
        transmilenio.carac_1="Rapido";
        transmilenio.carac_2="Peligroso";
        transmilenio.carac_3="Lleno";
        System.out.println("(1) Transmilenio:\n\n"+"Color:\t"+transmilenio.color+"\n"+"Tamaño(m):\t"+transmilenio.tamaño+"\n"+transmilenio.carac_1+"\n"+transmilenio.carac_2+"\n"+transmilenio.carac_3+"\n");
        
